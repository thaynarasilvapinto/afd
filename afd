digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	q3
	node [shape=point]
	qi
	node [shape=circle]
	qi -> q0
	q0 -> q1 [label=a]
	q0 -> q0 [label=b]
	q1 -> q2 [label=a]
	q1 -> q0 [label=b]
	q2 -> q3 [label=a]
	q2 -> q0 [label=b]
	q3 -> q3 [label=a]
	q3 -> q0 [label=b]
}
